import java.util.Scanner;
public class Main {
    public static void patternPrint(int rows) {
        for (int i = 1; i <= rows; ++i) {
          for (int j = 1; j <= i; ++j) {
             System.out.print("*");
          }
          System.out.println();
       }
    }
    public static void main(String args[]) {
        int rows;
        Scanner sc = new Scanner(System.in);
        rows = sc.nextInt();
        if(rows > 0) {
           patternPrint(rows);
        }
        else {
           System.out.print("Invalid Input");
        }
    }
}
